{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Iniciar",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "args": ["--no-timeouts", "--check-leaks"],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "npm: pretest",
      "runtimeExecutable": null,
      "runtimeArgs": ["--nolazy"],
      "env": {
        "NODE_ENV": "testing"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "sourceMaps": true
    }
  ]
}
