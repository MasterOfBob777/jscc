{
  "name": "jscc",
  "version": "0.3.5",
  "description": "The JavaScript preprocessor for conditional comments and replacements in text files",
  "main": "./lib/jscc.js",
  "module": "./lib/jscc.es.js",
  "jsnext:main": "./lib/jscc.es.js",
  "license": "MIT",
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- test/index.js",
    "lint": "eslint src test/index.js",
    "pretest": "tsc",
    "build": "rm -rf dist/* && tsc",
    "prepublish": "npm run lint && npm test && npm run build"
  },
  "keywords": [
    "html",
    "html5",
    "parser",
    "javascript",
    "es6",
    "conditional",
    "comments",
    "compilation",
    "preprocessor",
    "replacement",
    "jspreproc"
  ],
  "author": {
    "name": "aMarCruz",
    "email": "amarcruzbox-git@yahoo.com",
    "url": "https://github.com/aMarCruz"
  },
  "homepage": "https://github.com/aMarCruz/jscc",
  "bugs": "https://github.com/aMarCruz/jscc/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/aMarCruz/jscc"
  },
  "dependencies": {
    "magic-string": "^0.25.1",
    "tslib": "^1.9.3",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "rollup": "^0.66.0"
  }
}
